public class MACAPOBRE_ACTIVITY_I{
    public static void main(String[] args);
    int a = 10, b = 15, c = 20;

    int sum = a + b + c;
    int diff = a - b;
    int quo = b \ c;
    int pro = a * b;

    System.out.println("Sum: " + sum);
    System.out.println("Difference: " + diff);
    System.out.println("Quotient: " + quo);
    System.out.println("Product: " + pro);
}