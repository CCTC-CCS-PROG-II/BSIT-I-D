import java.util.Scanner;
public class MACAPOBRE_ACTIVITY_VI {

    public static void main (String []args){

        Scanner sc = new Scanner(System.in);
        System.out.println("Input a Number");
        int x = sc.nextInt();
        
        do{
        System.out.println(x);
        x--;
        }while(x>=0);
        
        
        
        
    }
        }